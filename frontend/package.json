{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "prepare": "svelte-kit sync && npm run smui-theme",
    "smui-theme": "smui-theme compile static/smui.css -i src/theme",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path ../.gitignore .",
    "format": "prettier --ignore-path ../.gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@smui-extra/autocomplete": "^6.0.0-beta.16",
    "@smui/button": "^6.0.0-beta.16",
    "@smui/dialog": "^6.0.0-beta.16",
    "@smui/layout-grid": "^6.0.0-beta.16",
    "@smui/textfield": "^6.0.0-beta.16",
    "@sveltejs/adapter-auto": "1.0.0-next.39",
    "@sveltejs/kit": "1.0.0-next.321",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.46.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.4.0",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "dependencies": {
    "@smui/circular-progress": "^6.0.0-beta.16",
    "@smui/list": "^6.0.0-beta.16",
    "cookie": "^0.5.0"
  }
}
